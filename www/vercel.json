{
    "version": 2,
    "builds": [
      {
        "src": "www/*/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "www/*/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "www/*/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.+)/(.+\\.html)",
        "dest": "/www/$1/$2"
      },
      {
        "src": "/(.+)/(.+\\.js)",
        "dest": "/www/$1/$2"
      },
      {
        "src": "/(.+)/(.+\\.ts)",
        "dest": "/www/$1/$2"
      },
      {
        "src": "/(.+)",
        "dest": "/www/$1/index.html"
      }
    ]
  }
  